	end)
	
	MakeTextInInput("_",true)
	
	task.spawn(function()
		while true do
			wait(0.3)
			if _Ipt == nil then
				return
			end
			
			if _Ipt then
				if _Ipt.Visible == false then
					_Ipt.Visible = true
				else
					_Ipt.Visible = false
				end
			end
		end
	end)
	
	task.spawn(function()
		
		local msg = "Minecraft chat simulator loaded successfully, by üíú‚ù§Ô∏èTr·∫ßnM·ªπNhi‚ù§Ô∏èüíú,Ver:1.0"
		
		local nmsg = script.Parent.MsgTemplate:Clone()
