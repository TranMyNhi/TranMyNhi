			StringToMsg(`<{msg.TextSource.Name or "[System]"}>{msg}`,nmsg)
		else
			StringToMsg(`<System>{string.gsub(msg, "<.-?>", "")}`,nmsg,Color3.fromRGB(255, 255, 0))
		end
	
	
		nmsg.Visible = true
		nmsg.Input.Visible = false
		if AtoSize == true then
			script.Parent.Msgs.CanvasPosition = Vector2.new(math.huge,math.huge)
		end
		wait()
		nmsgtxt.Visible = true
		script.Parent.Msgs.Visible = true
		local tttttt = nmsgtxt
		wait(5)
		if inping == false then
			tttttt.Visible = false
			if tttttt == nmsgtxt then
				script.Parent.Msgs.Visible = false
				for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
					if item:IsA("Frame") then
						item.Visible = false
					end
				end
			end
		end
	end)
	
	StringToMsg("Insert character",script.Parent.ChatInput.Ipt,Color3.fromRGB(0, 85, 0))
	script.Parent.ChatInput.Ipt.Input.Visible = false
	
	script.Parent.ChatInput.Ipt:GetPropertyChangedSignal("Visible"):Connect(function()
		if script.Parent.ChatInput.Ipt.Visible == false then
			script.Parent.ChatInput.Ipt.Visible = true
		end
	end)
	
