	local AllFontMapChars = {}
	
	for key, value in pairs(FontMap) do
		if type(key) == "string" and not tonumber(key) then
			AllFontMapChars[key] = value
		end
	end
	
	local oley4 = {".",",","i",";",":"}
	local oley6 = {"!","'","l"}
	local oley9 = {"f","t",`"`,"I"}
	local oley11 = {"u","-","+"}
	local oley18 = {"*"}
	
	local ptxt = script.Parent.ChatInput.Input
	local pchar = ">"
	local inputtexts = {}
	local View = {}
	local userinput = game:GetService("UserInputService")
	local inping = false
	local inputtxtsStr = ""
	local msgservice = game:GetService("TextChatService")
	local HWlkSpd = nil
	local HJpower = nil
	local PCamMax,PCamMin
	local AtoSize = false -- Deprecated
	local _Ipt = nil
	local nmsgtxt = nil
	local upper = false
	local chatHistory = {}
	local selectChatHistory = 1
	
	local function stringToTable(str)
		local charTable = {}
		for char in str:gmatch(".") do
			table.insert(charTable, char)
		end
		return charTable
	end
	
	local function utf8sub(s, i, j)
		i = utf8.offset(s, i)
		j = utf8.offset(s, j + 1)
		if not i or not j then
			return ""
		end
		return string.sub(s, i, j - 1)
	end
	
	local function StringToMapImageVector(char)
		if FontMap[char] then
			if FontMap[char] ~= nil then
				local Vector = Vector2.new(FontMap[char].X * 16,FontMap[char].Y * 16)
				
				if Vector then
					return Vector
				end
			end
		else
			return nil
		end
	end
	
	local function MakeTextInInput(char,IsIpt)
		
		if _Ipt then
			_Ipt:Destroy()
		end
		
		local txt = script.Parent.Template:Clone()
		txt.Parent = script.Parent.ChatInput
		local vet = StringToMapImageVector(tostring(char))
		if vet == nil then
			txt:Destroy()
			txt = script.Parent.Template2:Clone()
			txt.Parent = script.Parent.ChatInput
			txt.Text = char or "ÔøΩ"
			txt.Visible = true
			txt.Position = ptxt.Position + UDim2.new(0,16,0,0)
		end
		if vet ~= nil then
			txt.ImageRectOffset = vet
			txt.Visible = true
			txt.Position = ptxt.Position
			txt.Position = ptxt.Position + UDim2.new(0,12,0,0)
		end
		
		if table.find(oley4,pchar) then
			txt.Position = ptxt.Position + UDim2.new(0,4,0,0)
		end
		
		if table.find(oley6,pchar) then
			txt.Position = ptxt.Position + UDim2.new(0,6,0,0)
		end
		
		if table.find(oley9,pchar) then
			txt.Position = ptxt.Position + UDim2.new(0,9,0,0)
		end
		
		if table.find(oley11,pchar) then
			txt.Position = ptxt.Position + UDim2.new(0,11,0,0)
		end
		
		if table.find(oley18,pchar) then
			txt.Position = ptxt.Position + UDim2.new(0,18,0,0)
		end
		
		if IsIpt then
			if IsIpt == true then
				txt.Name = "_Ipt"
				_Ipt = txt
				return
			end
		end
		
		ptxt = txt
		pchar = char
		table.insert(inputtexts,txt)
		inputtxtsStr = inputtxtsStr .. char
		MakeTextInInput("_",true)
		
	end
	
	local function StringToMsg(Str,TargetUIObj,Color,pt,pc,rtpt)
		--print("MakeMsg:" .. tostring(Str))
		--print("#=" .. tostring(#Str))
		local ptxtinUI = TargetUIObj.Input
		local pcharinUI = ">"
		
		if pt then
			ptxtinUI = pt
		end
		
		if pc then
			pcharinUI = pc
		end
		
		local nowY = nil
		nowY = TargetUIObj.Input.Position.Y.Offset
		string.gsub(Str,"[%c]", "")
		
		Str = string.gsub(Str,"&lt;","<")
		Str = string.gsub(Str,"&gt;",">")
		
		local msgstr = stringToTable(Str)
		
		for i = 1,#Str do
			local txt = script.Parent.Template:Clone()
			
			local firsttxt = TargetUIObj.Input
			
			local vet = StringToMapImageVector(tostring(utf8sub(Str,i,i)))
			if vet == nil then
				txt:Destroy()
				txt = script.Parent.Template2:Clone()
				txt.Parent = script.Parent.ChatInput
				txt.Text = utf8sub(Str,i,i) or "ÔøΩ"
				txt.Visible = true
				txt.Position = ptxtinUI.Position + UDim2.new(0,12,0,0)
				if Color then
					txt.TextColor3 = Color
				end
				--print(tostring(i) .. ":")
				--print(utf8sub(Str,i,i))
				
			end
			if vet ~= nil then
				txt.ImageRectOffset = vet
				txt.Visible = true
				txt.Position = ptxtinUI.Position
				txt.Position = ptxtinUI.Position + UDim2.new(0,12,0,0)
			end
			txt.Parent = TargetUIObj
			if table.find(oley4,pcharinUI) then
				txt.Position = ptxtinUI.Position + UDim2.new(0,4,0,0)
			end
	
			if table.find(oley6,pcharinUI) then
				txt.Position = ptxtinUI.Position + UDim2.new(0,6,0,0)
			end
	
			if table.find(oley9,pcharinUI) then
				txt.Position = ptxtinUI.Position + UDim2.new(0,9,0,0)
			end
	
			if table.find(oley11,pcharinUI) then
				txt.Position = ptxtinUI.Position + UDim2.new(0,11,0,0)
			end
	
			if table.find(oley18,pcharinUI) then
				txt.Position = ptxtinUI.Position + UDim2.new(0,18,0,0)
			end
			
			if txt.Position.X.Offset >= TargetUIObj.Size.X.Offset + 400 then
				nowY += 20
				txt.Position = UDim2.new(0,5,0,nowY)
			end
			
			if Color and txt:IsA("ImageLabel") then
				txt.ImageColor3 = Color
			end
			
			ptxtinUI = txt
			pcharinUI = utf8sub(Str,i,i)
			
		end
		
		nmsgtxt = TargetUIObj
		
		if rtpt then
			if rtpt == true then
				return ptxtinUI,pcharinUI
			end
		end
	end
	
	--ÔøΩ=17
	--AB,ab=12
	
	local specialKeyMapping = {
		[Enum.KeyCode.LeftBracket] = "[",
		[Enum.KeyCode.RightBracket] = "]",
		[Enum.KeyCode.Semicolon] = ";",
		[Enum.KeyCode.Quote] = "'",
		[Enum.KeyCode.Comma] = ",",
		[Enum.KeyCode.Period] = ".",
		[Enum.KeyCode.Slash] = "/",
		[Enum.KeyCode.BackSlash] = "\\",
		[Enum.KeyCode.Minus] = "-",
		[Enum.KeyCode.Equals] = "=",
		[Enum.KeyCode.Tilde] = "`"
	}
	
	local shiftSpecialCharacters = {
		[Enum.KeyCode.One] = "!",
		[Enum.KeyCode.Two] = "@",
		[Enum.KeyCode.Three] = "#",
		[Enum.KeyCode.Four] = "$",
		[Enum.KeyCode.Five] = "%",
		[Enum.KeyCode.Six] = "^",
		[Enum.KeyCode.Seven] = "&",
		[Enum.KeyCode.Eight] = "*",
		[Enum.KeyCode.Nine] = "(",
		[Enum.KeyCode.Zero] = ")",
		[Enum.KeyCode.Minus] = "_",
		[Enum.KeyCode.Equals] = "+",
		[Enum.KeyCode.LeftBracket] = "{",
		[Enum.KeyCode.RightBracket] = "}",
		[Enum.KeyCode.BackSlash] = "|",
		[Enum.KeyCode.Semicolon] = ":",
		[Enum.KeyCode.Quote] = "\"",
		[Enum.KeyCode.Comma] = "<",
		[Enum.KeyCode.Period] = ">",
		[Enum.KeyCode.Slash] = "?",
		[Enum.KeyCode.Tilde] = "~"
	}
	
	local numberKeyMapping = {
		[Enum.KeyCode.Zero] = "0",
		[Enum.KeyCode.One] = "1",
		[Enum.KeyCode.Two] = "2",
		[Enum.KeyCode.Three] = "3",
		[Enum.KeyCode.Four] = "4",
		[Enum.KeyCode.Five] = "5",
		[Enum.KeyCode.Six] = "6",
		[Enum.KeyCode.Seven] = "7",
		[Enum.KeyCode.Eight] = "8",
		[Enum.KeyCode.Nine] = "9"
	}
	
	local keyPadMapping = {
		[Enum.KeyCode.KeypadZero] = "0",
		[Enum.KeyCode.KeypadOne] = "1",
		[Enum.KeyCode.KeypadTwo] = "2",
		[Enum.KeyCode.KeypadThree] = "3",
		[Enum.KeyCode.KeypadFour] = "4",
		[Enum.KeyCode.KeypadFive] = "5",
		[Enum.KeyCode.KeypadSix] = "6",
		[Enum.KeyCode.KeypadSeven] = "7",
		[Enum.KeyCode.KeypadEight] = "8",
		[Enum.KeyCode.KeypadNine] = "9",
		[Enum.KeyCode.KeypadPeriod] = ".",
		[Enum.KeyCode.KeypadMultiply] = "*",
		[Enum.KeyCode.KeypadDivide] = "/",
		[Enum.KeyCode.KeypadPlus] = "+",
		[Enum.KeyCode.KeypadMinus] = "-",
		[Enum.KeyCode.KeypadEquals] = "="
	}
	
	for key,Value in pairs(AllFontMapChars) do
		local nt = script.Parent.TemplateBtn:Clone()
		nt.Parent = script.Parent.ChatInput.IptChar
		nt.ImageRectOffset = StringToMapImageVector(key)
		nt.Visible = true
		nt.MouseButton1Click:Connect(function()
			MakeTextInInput(key)
		end)
	end
	
	script.Parent.ChatInput.Input.ImageRectOffset = StringToMapImageVector(">")
	
	userinput.InputBegan:Connect(function(input,pro)
		
		if input.KeyCode == Enum.KeyCode.CapsLock then
			if upper == true then
				upper = false
				script.Parent.EbUpper.BackgroundColor3 = Color3.fromRGB(255,0,0)
			else
				upper = true
				script.Parent.EbUpper.BackgroundColor3 = Color3.fromRGB(0,255,0)
			end
		end
		
		if input.KeyCode == Enum.KeyCode.Slash then
			if inping == false then
				inping = true
				script.Parent.ChatInput.Visible = true
				script.Parent.Msgs.Visible = true
				script.Parent.EbUpper.Visible = true
				HWlkSpd = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
				HJpower = game.Players.LocalPlayer.Character.Humanoid.JumpPower
				PCamMax,PCamMin = game.Players.LocalPlayer.CameraMaxZoomDistance,game.Players.LocalPlayer.CameraMinZoomDistance
				game.Players.LocalPlayer.CameraMaxZoomDistance,game.Players.LocalPlayer.CameraMinZoomDistance = 30,30
				
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
				game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
				for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
					if item:IsA("Frame") then
						item.Visible = true
					end
				end
				script.Parent.Msgs.CanvasPosition = Vector2.new(math.huge,math.huge)
	
			end
		end
		
		
		if inping == false then
			return
		end
		
		if input.UserInputType == Enum.UserInputType.Keyboard then
			local key = input.KeyCode
	
			if userinput:IsKeyDown(Enum.KeyCode.LeftShift) or userinput:IsKeyDown(Enum.KeyCode.RightShift) then
				if shiftSpecialCharacters[key] then
					MakeTextInInput(shiftSpecialCharacters[key])
					wait(0.5)
					while userinput:IsKeyDown(key) do
						wait(0.01)
						MakeTextInInput(shiftSpecialCharacters[key])
					end
					return
				end
			end
	
	
			if key.Value >= Enum.KeyCode.A.Value and key.Value <= Enum.KeyCode.Z.Value then
				local isShiftDown = userinput:IsKeyDown(Enum.KeyCode.LeftShift) or userinput:IsKeyDown(Enum.KeyCode.RightShift) or upper == true
				if isShiftDown then
					MakeTextInInput(key.Name)
					wait(0.5)
					while userinput:IsKeyDown(key) do
						wait(0.01)
						MakeTextInInput(key.Name)
					end
				else
					MakeTextInInput(key.Name:lower())
					wait(0.5)
					while userinput:IsKeyDown(key) do
						wait(0.01)
						MakeTextInInput(key.Name:lower())
					end
				end
			elseif key.Value >= Enum.KeyCode.Zero.Value and key.Value <= Enum.KeyCode.Nine.Value then
				MakeTextInInput(tostring(numberKeyMapping[key]))
				wait(0.5)
				while userinput:IsKeyDown(key) do
					wait(0.01)
					MakeTextInInput(tostring(numberKeyMapping[key]))
				end
			end
			
			if keyPadMapping[key] then
				MakeTextInInput(keyPadMapping[key])
				wait(0.5)
				while userinput:IsKeyDown(key) do
					wait(0.01)
					MakeTextInInput(keyPadMapping[key])
				end
			end
			
			if specialKeyMapping[key] then
				MakeTextInInput(specialKeyMapping[key])
				wait(0.5)
				while userinput:IsKeyDown(key) do
					wait(0.01)
					MakeTextInInput(specialKeyMapping[key])
				end
			end
	
			if input.KeyCode == Enum.KeyCode.Up then
				print("up")
					selectChatHistory = selectChatHistory - 1
					if selectChatHistory <= 0 then
						selectChatHistory = #chatHistory
					end
					
					while #inputtexts > 0 do
					if #inputtexts > 0 then
						inputtexts[#inputtexts]:Destroy()
						table.remove(inputtexts,#inputtexts)
						inputtxtsStr = utf8sub(inputtxtsStr,1,#inputtxtsStr - 1)
						ptxt = inputtexts[#inputtexts]
						pchar = utf8sub(inputtxtsStr,#inputtxtsStr,#inputtxtsStr)
	
						if _Ipt then
							_Ipt:Destroy()
						end
	
						if #inputtexts == 0 then
							pchar = ">"
							ptxt = script.Parent.ChatInput.Input
						end
	
						MakeTextInInput("_",true)
					end
					end
					
				for i = 1,#chatHistory[selectChatHistory] do
					if utf8sub(chatHistory[selectChatHistory],i,i) ~= "" then
						MakeTextInInput(utf8sub(chatHistory[selectChatHistory],i,i))
					end
				end
			end
			
			if input.KeyCode == Enum.KeyCode.Down then
				print("up")
				selectChatHistory = selectChatHistory + 1
				if selectChatHistory > #chatHistory then
					selectChatHistory = 1
				end
	
				while #inputtexts > 0 do
					if #inputtexts > 0 then
						inputtexts[#inputtexts]:Destroy()
						table.remove(inputtexts,#inputtexts)
						inputtxtsStr = utf8sub(inputtxtsStr,1,#inputtxtsStr - 1)
						ptxt = inputtexts[#inputtexts]
						pchar = utf8sub(inputtxtsStr,#inputtxtsStr,#inputtxtsStr)
	
						if _Ipt then
							_Ipt:Destroy()
						end
	
						if #inputtexts == 0 then
							pchar = ">"
							ptxt = script.Parent.ChatInput.Input
						end
	
						MakeTextInInput("_",true)
					end
				end
	
				for i = 1,#chatHistory[selectChatHistory] do
					if utf8sub(chatHistory[selectChatHistory],i,i) ~= "" then
						MakeTextInInput(utf8sub(chatHistory[selectChatHistory],i,i))
					end
				end
			end
			
			
			if input.KeyCode == Enum.KeyCode.Space then
				MakeTextInInput(" ")
				
				wait(0.5)
				while userinput:IsKeyDown(key) do
					wait(0.01)
					MakeTextInInput(" ")
				end
			end
			
			if input.KeyCode == Enum.KeyCode.Backspace then
				
				if #inputtexts > 0 then
					inputtexts[#inputtexts]:Destroy()
					table.remove(inputtexts,#inputtexts)
					inputtxtsStr = utf8sub(inputtxtsStr,1,#inputtxtsStr - 1)
					ptxt = inputtexts[#inputtexts]
					pchar = utf8sub(inputtxtsStr,#inputtxtsStr,#inputtxtsStr)
					
					
					if _Ipt then
						_Ipt:Destroy()
					end
					
					if #inputtexts == 0 then
						pchar = ">"
						ptxt = script.Parent.ChatInput.Input
					end
					
					MakeTextInInput("_",true)
				end
				
				wait(0.5)
				while userinput:IsKeyDown(key) do
					wait(0.01)
					if #inputtexts > 0 then
						inputtexts[#inputtexts]:Destroy()
						table.remove(inputtexts,#inputtexts)
						inputtxtsStr = utf8sub(inputtxtsStr,1,#inputtxtsStr - 1)
						ptxt = inputtexts[#inputtexts]
						pchar = utf8sub(inputtxtsStr,#inputtxtsStr,#inputtxtsStr)
	
						if _Ipt then
							_Ipt:Destroy()
						end
	
						if #inputtexts == 0 then
							pchar = ">"
							ptxt = script.Parent.ChatInput.Input
						end
	
						MakeTextInInput("_",true)
					end
				end
			end
			
			if input.KeyCode == Enum.KeyCode.Return or input.KeyCode == Enum.KeyCode.KeypadEnter then			
				local sendtxt = inputtxtsStr
				
				
				
				for i, item in pairs(inputtexts) do
					item:Destroy()
				end
				
				inputtexts = {}
				inputtxtsStr = ""
				ptxt = ">"
				ptxt = script.Parent.ChatInput.Input
				inping = false
				script.Parent.ChatInput.Visible = false
				script.Parent.Msgs.Visible = false
				script.Parent.EbUpper.Visible = false
				
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = HWlkSpd or game.StarterPlayer.CharacterWalkSpeed
				game.Players.LocalPlayer.Character.Humanoid.JumpPower = HJpower or game.StarterPlayer.CharacterJumpPower
				game.Players.LocalPlayer.CameraMaxZoomDistance,game.Players.LocalPlayer.CameraMinZoomDistance = PCamMax,PCamMin
				
				if sendtxt == "" then
					return
				end
				table.insert(chatHistory,sendtxt)
				game.TextChatService.TextChannels.RBXGeneral:SendAsync(tostring(sendtxt) or " ")
				wait()
				for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
					if item:IsA("Frame") then
						item.Visible = false
					end
				end
				
				selectChatHistory = #chatHistory + 1
			end
		end
	end)
	
	msgservice.MessageReceived:Connect(function(msg)
		
		if msg.Text == "" then
			return
		end
		
		local nmsg = script.Parent.MsgTemplate:Clone()
		nmsg.Parent = script.Parent.Msgs
		
		if msg.TextSource then
			wait(0.5)
			local plr = game.Players:FindFirstChild(msg.TextSource.Name)
			local clr = Color3.fromRGB(255,255,255)
			
			if plr then
				if plr:IsA("Player") then
					clr = plr.TeamColor.Color
				end
			end
			
			local pt,pc = StringToMsg(msg.TextSource.Name,nmsg,clr,nil,nil,true)
			wait()
			StringToMsg(`: {msg.Text}`,nmsg,nil,pt,pc,nil)
		else
			wait(0.5)
			if inping == false then
				for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
					if item:IsA("Frame") then
						item.Visible = false
					end
				end
			end
			StringToMsg(`<System>{string.gsub(msg.Text, "<.-?>", "")}`,nmsg,Color3.fromRGB(255, 255, 0))
			
		end
		
		
		
		nmsg.Visible = true
		nmsg.Input.Visible = false
		if AtoSize == true then
			script.Parent.Msgs.CanvasPosition = Vector2.new(math.huge,math.huge)
		end
		table.insert(View,nmsgtxt)
		wait()
		for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
			if item:IsA("Frame") then
				
				if table.find(View,item) then
					
				else
				
				end
			end
		end
		
		script.Parent.Msgs.Visible = true
		local tttttt = nmsgtxt
		
		wait(5)
		if inping == false then
			tttttt.Visible = false
			if tttttt == nmsgtxt then
				script.Parent.Msgs.Visible = false
				for i, item in ipairs(script.Parent.Msgs:GetChildren()) do
					if item:IsA("Frame") then
						item.Visible = false
					end
				end
				
				for i, item in pairs(View) do
					if item == tttttt then
						table.remove(View,i)
					end
				end
			end
		end
	end)
	
	game.Players.PlayerAdded:Connect(function(plr)
	
		local nmsg = script.Parent.MsgTemplate:Clone()
		nmsg.Parent = script.Parent.Msgs
	
		StringToMsg(plr.Name .. "joined the game.",nmsg, Color3.fromRGB(255, 255, 0))
	
		nmsg.Visible = true
		nmsg.Input.Visible = false
